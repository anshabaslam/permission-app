{
  "expo": {
    "name": "Permission Pro",
    "slug": "permission-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "description": "Secure permission management app for Android and iOS devices. Easily manage camera, location, photos, and SMS permissions with a clean, intuitive interface.",
    "privacy": "public",
    "splash": {
      "image": "./assets/logo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#F3F0FF"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to scan receipts",
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to track transactions",
        "NSPhotoLibraryUsageDescription": "This app needs access to photos to organize receipts"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#F3F0FF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.permission.pro",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_SMS",
        "android.permission.RECEIVE_SMS"
      ],
      "allowBackup": false,
      "usesCleartextTraffic": false
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "owner": "anshab00",
    "extra": {
      "eas": {
        "projectId": "eec2fda4-a1a8-4357-8fcd-f02755d0ebcd"
      }
    }
  }
}
